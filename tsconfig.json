{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "outDir": "prod-server",
    "importHelpers": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    },
  },
  "include": ["dev-server/**/*", "dev-server/locales/**/*.json"]
}
